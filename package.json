{
  "name": "gorae-server",
  "version": "0.7.0",
  "description": "docker remote api proxy",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rhiokim/gorae-server.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git cz",
    "pretest": "npm run lint",
    "posttest": "nsp check",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty '**/*.@(js)'",
    "lint:msg": "validate-commit-msg",
    "start": "node ."
  },
  "author": "rhiokim",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.2.1",
    "bluebird": "^3.4.6",
    "cors": "^2.8.1",
    "dockerode": "^2.3.1",
    "event-stream": "^3.3.4",
    "express": "^4.14.0",
    "express-ws": "^2.0.0-rc.1",
    "got": "^6.6.3",
    "http-proxy": "^1.15.2",
    "socket.io": "^1.5.1"
  },
  "devDependencies": {
    "eslint": "3.8.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "express-routemap": "^1.0.2",
    "server-sent-events": "^1.0.0",
    "commitizen": "^2.8.6",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "validate-commit-msg": "^2.8.2",
    "nsp": "^2.6.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "npm run lint:msg"
    },
    "validate-commit-msg": {
      "preset": "angular",
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
